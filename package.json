{
  "name": "getting-started-with-express",
  "version": "1.0.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "npm-run-all clean lint tsc copy-assets",
    "tsc": "tsc",
    "prestart": "npm run build",
    "start": "node .",
    "start:dev": "npm-run-all build start",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run start:dev",
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node src/tools/copy-assets && npm run to-css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "to-css": "node-sass src/scss/styles.scss -o dist/views/css"
  },
  "author": "ali.chouenyib",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.25.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.1",
    "mongoose-unique-validator": "^3.0.0",
    "mysql": "^2.18.1",
    "redis": "^4.0.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.17",
    "@types/shelljs": "^0.8.11",
    "fs-extra": "^10.0.0",
    "node-sass": "^7.0.1",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.5",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  }
